/**
 * 8-bit bitwise And:
 * for i = 0..7: out[i] = (a[i] and b[i])
 */

CHIP And8 {
    IN a[8], b[8];
    OUT out[8];

    PARTS:
    Not8 (in=a, out=nota);
    Not8 (in=b, out=notb);
    Or8 (a=nota, b=notb, out=res);
    Not8 (in=res, out=out);
    
/*
   PARTS:
   Nand8(a=a, b=b, out=nand);
   Not8(in=nand, out=out);
 */
}
